algoritmo "Estrutura-Para-Faça"
var
    numero, fat, cont: Inteiro
inicio
    Escreva ("Digite o número a ser fatorado: ")
    Leia (numero)
    
    fat := 1
    Para Cont de 1 até numero faca
        fat := fat * cont
    FimPara
    Escreva ("A fatorial do número ", numero, " é: ",Fat)
fimalgoritmo